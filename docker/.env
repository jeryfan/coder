# Coder 版本
CODER_VERSION=latest

# 网络配置
CODER_HTTP_PORT=7080
# 浏览器访问 Coder 的地址，生产环境应设为实际域名（如 https://coder.example.com）
CODER_ACCESS_URL=http://localhost:7080
# 通配符域名，用于端口转发等功能（如 *.coder.example.com），可留空
CODER_WILDCARD_ACCESS_URL=
# 是否将 Host 不匹配 CODER_ACCESS_URL 的请求重定向到 CODER_ACCESS_URL
# Docker 部署时建议设为 false，否则 workspace 容器通过内部 DNS (coder-server) 访问时
# 会被重定向到 CODER_ACCESS_URL，导致 agent 无法下载和连接
# 生产环境使用域名 + 反向代理时可设为 true
CODER_REDIRECT_TO_ACCESS_URL=false

# PostgreSQL 配置
POSTGRES_USER=coder
POSTGRES_PASSWORD=coder
POSTGRES_DB=coder

# Docker 配置
DOCKER_GID=0

# Coder 高级配置
CODER_LOG_LEVEL=info
CODER_SWAGGER_ENABLE=true
